    "def decimal_to_binary():\n",
    "    num = int(input(\"Enter a decimal number: \"))\n",
    "    binary = bin(num)[2:]  \n",
    "    print(f\"Binary equivalent: {binary}\")\n",
    "\n",
    "decimal_to_binary()\n"
