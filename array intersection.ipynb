    "import numpy as np \n",
    "x1=np.array([10,20,30,90])\n",
    "x2=np.array([20,30,100,0])\n",
    "def arr_common(x1,x2):\n",
    "    new_arr=[]\n",
    "    for i in x1:\n",
    "        if i in x2:\n",
    "            new_arr.append(i)\n",
    "    return np.array(new_arr)\n",
    "res=arr_common(x1,x2)\n",
    "print(type(res))\n",
    "print(res)"
   ]
